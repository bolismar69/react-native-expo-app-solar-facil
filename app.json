{
  "expo": {
    "name": "Solar Facil",
    "slug": "solar-facil",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/solar-facil-icon.png",
    "scheme": "solar-facil",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.janeves.solarfacil",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/solar-facil-adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.janeves.solarfacil"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/solar-facil-favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/solar-facil-splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "eaf32393-0c03-4e40-9005-fd5955794e5a"
      }
    },
    "owner": "bolismar69",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/eaf32393-0c03-4e40-9005-fd5955794e5a"
    }
  }
}
